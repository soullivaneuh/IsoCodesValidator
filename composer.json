{
    "name": "sllh/iso-codes-validator",
    "type": "library",
    "description": "Symfony validator wrapper of ronanguilloux/isocodes",
    "keywords": ["ISO codes", "Symfony", "Validator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan Senechal",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": "^5.4|^7.0",
        "symfony/validator": "^2.3.20|^3.0",
        "symfony/translation": "^2.3|^3.0",
        "ronanguilloux/isocodes": "^1.2"
    },
    "require-dev": {
        "symfony/dependency-injection": "^2.3|^3.0",
        "symfony/http-kernel": "^2.3|^3.0",
        "symfony/finder": "^2.3|^3.0",
        "silex/silex": "^1.1",
        "matthiasnoback/symfony-dependency-injection-test": "^0.7.4",
        "satooshi/php-coveralls": "^1.0",
        "codeclimate/php-test-reporter": "dev-master",
        "symfony/phpunit-bridge": "^2.7|^3.0",
        "sllh/php-cs-fixer-styleci-bridge": "^1.0"
    },
    "suggest": {
        "symfony/dependency-injection": "For Symfony integration as a bundle",
        "symfony/http-kernel": "For Symfony integration as a bundle",
        "symfony/finder": "For Symfony integration as a bundle",
        "silex/silex": "For Silex integration as a service provider"
    },
    "conflict": {
        "symfony/dependency-injection": "<2.3|>=4.0",
        "symfony/http-kernel": "<2.3|>=4.0",
        "symfony/finder": "<2.3|>=4.0",
        "silex/silex": "<1.1.1|>=2.0"
    },
    "autoload": {
        "psr-4": { "SLLH\\IsoCodesValidator\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "SLLH\\IsoCodesValidator\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
